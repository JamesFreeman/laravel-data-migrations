{
  "name": "jamesfreeman/laravel-data-migrations",
  "description": "An extension to allow you to separate data migrations from structure migrations",
  "license": "MIT",
  "authors": [
    {
      "name": "Jose Lorente",
      "email": "jose.lorente.martin@gmail.com"
    },
    {
      "name": "Eran Machiels",
      "email": "dev@eranmachiels.nl"
    },
    {
      "name": "James Freeman",
      "email": "me@james-freeman.info"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0|^9.0",
    "illuminate/database": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "JamesFreeman\\DataMigrations\\": "src/"
    }
  },
  "extra": {
    "laravel": {
